{
  "expo": {
    "name": "Income Tax Tracker",
    "slug": "income-tax-tracker",
    "version": "1.0.1",
    "orientation": "portrait",
    "sdkVersion": "54.0.0",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#4338CA"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.incometaxtracker.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#4338CA"
      },
      "package": "com.patch1d.incometaxtracker",
      "versionCode": 2,
      "permissions": [
        "READ_SMS",
        "RECEIVE_SMS"
      ]
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "buildToolsVersion": "35.0.0",
            "enableProguardInReleaseBuilds": true,
            "enableShrinkResourcesInReleaseBuilds": true
          }
        }
      ],
      "./plugins/withSMSListener.js"
    ],
    "extra": {
      "eas": {
        "projectId": "e4a836ef-186c-4604-baa4-2aef91a5457a"
      },
      "googleSignIn": {
        "androidClientId": "287763222921-pffl6vs5vmj90sdu6nhqda9bldk532br.apps.googleusercontent.com",
        "iosClientId": "YOUR_IOS_CLIENT_ID_HERE.apps.googleusercontent.com",
        "webClientId": "287763222921-pffl6vs5vmj90sdu6nhqda9bldk532br.apps.googleusercontent.com"
      }
    }
  }
}
